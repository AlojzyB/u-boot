#include "stm32mp13-u-boot.dtsi"

/ {
	aliases {
		mmc1 = &sdmmc1;
		usb0 = &usbotg_hs;
	};

	config {
		u-boot,boot-led = "USER1";
		u-boot,error-led = "USER2";
	};
};

&adc_1 {
	status = "okay";
};

&uart5 {
	u-boot,dm-pre-reloc;
};

&uart5_pins_a {
	u-boot,dm-pre-reloc;
	pins1 {
		u-boot,dm-pre-reloc;
	};
	pins2 {
		u-boot,dm-pre-reloc;
	};
};
